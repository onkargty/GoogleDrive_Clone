{
  "name": "google-drive-clone",
  "version": "1.0.0",
  "description": "A full-featured Google Drive clone with React frontend and Node.js backend",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=frontend",
    "preview": "npm run preview --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build:backend": "npm run build --workspace=backend",
    "start:backend": "npm run start --workspace=backend",
    "install:all": "npm install && npm install --workspace=frontend && npm install --workspace=backend"
  },
  "keywords": [
    "google-drive",
    "file-storage",
    "react",
    "nodejs",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}